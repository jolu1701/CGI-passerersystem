<Window x:Class="Test.StartScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test"
        mc:Ignorable="d"
        Title="StartScreen" Height="400" Width="600">
    <Grid>
        <Frame Content="" HorizontalAlignment="Left" Height="181" Margin="32,79,0,0" VerticalAlignment="Top" Width="522" Background="#FFE2E2E2" BorderBrush="#FF494949" BorderThickness="1"/>

        <Button x:Name="btnToGuest" Content="Skriv in mig" HorizontalAlignment="Left" Margin="234,133,0,0" VerticalAlignment="Top" Width="110" Click="btnToGuest_Click" Height="41" BorderBrush="#FF3C3C3C" Background="#FFBFBFBF" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="btnToAdmin" Content="Logga in" HorizontalAlignment="Left" Margin="234,197,0,0" VerticalAlignment="Top" Width="110" Click="btnToAdmin_Click" Height="41" BorderBrush="#FF3C3C3C" Background="#FFBFBFBF" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="btnadminnologin" Content="" HorizontalAlignment="Left" Margin="566,342,0,0" VerticalAlignment="Top" Width="16" Click="btnadminnologin_Click" Height="17"/>
        <Label Content="Välkommen" HorizontalAlignment="Left" Margin="234,79,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="18"/>
        <Image HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="133" Source="Image/cgi-logo-.png" RenderOptions.BitmapScalingMode="HighQuality" Margin="0,-21,0,0"/>

    </Grid>
</Window>
